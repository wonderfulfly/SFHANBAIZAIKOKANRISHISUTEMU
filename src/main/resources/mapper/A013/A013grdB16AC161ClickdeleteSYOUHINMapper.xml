<mapper namespace="com.wonderrobot.sfhanbaizaikokanrishisutemu.mapper.A013.A013grdB16AC161ClickdeleteSYOUHINMapper">
	<resultMap type="com.wonderrobot.sfhanbaizaikokanrishisutemu.entity.A013.A013grdB16AC161ClickdeleteSYOUHINEntity" id="A013grdB16AC161ClickdeleteSYOUHINEntityMap">
	</resultMap>
	<!-- データ操作2:テーブル「商品」の項目「商品NO」が<結合文字2>のデータを削除する -->
	<delete id="A013grdB16AC161ClickdeleteSYOUHIN" >
		DELETE FROM SYOUHIN
			<where>
				 1 = 1 
				<if test="KETSUGOUMOJI2 != null and KETSUGOUMOJI2 != ''">
					 AND SHOUHINNO  = #{KETSUGOUMOJI2} 
				</if>
			</where>
	</delete>
</mapper>